#include <iostream>
using namespace std;

void citestemp(int a[][20],int&n)
{int i,j;
cout<<"n= ";
cin>>n;
for(i=0;i<n;i++)
for(j=0;j<n;j++)
{cout<<"a["<<i+1<<"]["<<j+1<<"]= ";
cin>>a[i][j];}}

void afisaremp(int a[][20],int n)
{int i,j;
for(i=0;i<n;i++)
{for(j=0;j<n;j++)
cout<<a[i][j]<<" ";
cout<<endl;}}

int impare(int a[][20],int n)
{int i,j, nr=0;
for(i=0;i<n;i++)
if(a[i][j]%2!=0)
nr++;
return nr;}
int main()
{int a[20][20],n;
citestemp(a,n);
afisaremp(a,n);
cout<<"pe diag princ exista "<<impare(a,n)<<" val impare";
return 0;}
